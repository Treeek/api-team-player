{
  "name": "api-team-player",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "swagger": "tsoa swagger",
    "prestart": "npm run build",
    "start": "node --require ./config .",
    "predev": "npm run swagger && npm run build",
    "dev": "node --require ./config .",
    "dev:watch": "nodemon",
    "i18n": "grunt i18n",
    "test": "jest --coverage --verbose"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.26",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/express-jwt": "0.0.42",
    "@types/helmet": "0.0.43",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/supertest": "^2.0.7",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/validator": "^10.11.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-validator": "^5.3.1",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.10.0",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.0.2",
    "tsoa": "^2.3.81",
    "typeorm": "^0.2.17",
    "typescript": "^3.4.5",
    "winston": "^3.2.1"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "grunt": "^1.0.4",
    "grunt-i18n-static": "^0.1.14",
    "jest": "^24.8.0",
    "mysql": "^2.17.1",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.0",
    "static-i18n": "^0.2.2",
    "ts-jest": "^24.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "dist"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
